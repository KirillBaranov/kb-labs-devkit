    "./eslint/node.js": "./eslint/node.js",
    "./eslint/react.js": "./eslint/react.js",
    "./eslint/node": "./eslint/node.js",
    "./eslint/react": "./eslint/react.js",
    "./prettier/": "./prettier/",
    "./prettier/index.json": "./prettier/index.json",
    "./vitest/": "./vitest/",
    "./vitest/node.js": "./vitest/node.js",
    "./vitest/react.js": "./vitest/react.js",
    "./vitest/node": "./vitest/node.js",
    "./vitest/react": "./vitest/react.js",
    "./vite/": "./vite/",
    "./vite/react-app.js": "./vite/react-app.js",
    "./vite/react-app": "./vite/react-app.js",
    "./tsup/": "./tsup/",
    "./tsup/node.js": "./tsup/node.js",
    "./tsup/react-lib.js": "./tsup/react-lib.js",
    "./tsup/bin.js": "./tsup/bin.js",
    "./tsup/sdk.js": "./tsup/sdk.js",
    "./tsup/node": "./tsup/node.js",
    "./tsup/react-lib": "./tsup/react-lib.js",
    "./tsup/bin": "./tsup/bin.js",
    "./tsup/sdk": "./tsup/sdk.js",
    "./agents/": "./agents/",
    "./cursorrules": "./.cursorrules",
    "./.vscode/settings.json": "./.vscode/settings.json",
    "./AGENTS.md": "./AGENTS.md",
