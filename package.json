{
  "name": "@kb-labs/devkit",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Shared developer toolkit for KB Labs projects: TS/ESLint/Prettier/Vitest/Tsup presets and reusable GitHub Actions.",
  "files": [
    "agents",
    ".cursorrules",
    "tsconfig",
    "eslint",
    "prettier",
    "vitest",
    "tsup",
    ".github",
    ".vscode",
    "bin",
    "sync",
    "AGENTS.md",
    ".github/workflow-templates/**",
    ".github/actions/**"
  ],
  "bin": {
    "kb-devkit-sync": "./bin/devkit-sync.mjs"
  },
  "exports": {
    "./tsconfig/": "./tsconfig/",
    "./tsconfig/base.json": "./tsconfig/base.json",
    "./tsconfig/cli.json": "./tsconfig/cli.json",
    "./tsconfig/lib.json": "./tsconfig/lib.json",
    "./tsconfig/node.json": "./tsconfig/node.json",
    "./tsconfig/test.json": "./tsconfig/test.json",
    "./eslint/": "./eslint/",
    "./eslint/node.js": "./eslint/node.js",
    "./eslint/node": "./eslint/node.js",
    "./prettier/": "./prettier/",
    "./prettier/index.json": "./prettier/index.json",
    "./vitest/": "./vitest/",
    "./vitest/node.js": "./vitest/node.js",
    "./vitest/node": "./vitest/node.js",
    "./tsup/": "./tsup/",
    "./tsup/node.js": "./tsup/node.js",
    "./tsup/node": "./tsup/node.js",
    "./agents/": "./agents/",
    "./cursorrules": "./.cursorrules",
    "./.vscode/settings.json": "./.vscode/settings.json",
    "./AGENTS.md": "./AGENTS.md",
    "./bin/devkit-sync.mjs": "./bin/devkit-sync.mjs",
    "./sync": "./sync/index.mjs",
    "./workflows-templates": "./workflows-templates/"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "echo \"@kb-labs/devkit: nothing to build\"",
    "fixtures": "node scripts/fixtures.js –help",
    "fixtures:check": "node scripts/fixtures.js –action=check",
    "fixtures:lint": "node scripts/fixtures.js –action=lint",
    "fixtures:test": "node scripts/fixtures.js –action=test",
    "fixtures:build": "node scripts/fixtures.js –action=build",
    "fixtures:bootstrap": "node scripts/fixtures.js –action=bootstrap",
    "fixtures:clean": "node scripts/fixtures.js –action=clean",
    "fixtures:ci": "node scripts/fixtures.js –action=check –since=${GITHUB_BASE_REF:-origin/main} –concurrency=3 || node scripts/fixtures.js –action=check"
  },
  "peerDependencies": {
    "eslint": "^9.35.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vitest": "^3.2.4"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-import": {
      "optional": true
    },
    "eslint-import-resolver-typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.35.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@9.11.0+sha512.0a203ffaed5a3f63242cd064c8fb5892366c103e328079318f78062f24ea8c9d50bc6a47aa3567cabefd824d170e78fa2745ed1f16b132e16436146b7688f19b",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.11.0"
  }
}